{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["lib"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "logo": {
      "url": "../.assets/logo_white.png",
      "width": "150px",
      "height": "63px",
      "link": "https://github.com/GV14982/async-airtable"
    },
    "footerText": "My awesome footer text"
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/docdash"
  },
  "docdash": {}
}
